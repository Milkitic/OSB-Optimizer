<Window x:Class="Milky.OsbOptimizer.Windows.DetailWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Milky.OsbOptimizer.Windows"
        xmlns:viewModels="clr-namespace:Milky.OsbOptimizer.ViewModels"
        xmlns:converters="clr-namespace:Milky.OsbOptimizer.Converters"
        mc:Ignorable="d"
        Title="DetailWindow" Height="450" Width="800"
        Loaded="Window_Loaded">
     <!--<Window.DataContext>
        <viewModels:OptimizeViewModel />
    </Window.DataContext>-->
    <Window.Resources>
        <converters:StringBuilderToStringConverter x:Key="StringBuilderToStringConverter" />
    </Window.Resources>
    <Grid>
        <TextBox x:Name="TbSituationOutput"
                 AcceptsReturn="True"
                 Text="{Binding SituationOutput,Converter={StaticResource StringBuilderToStringConverter}}" />

    </Grid>
</Window>
